<h1>List of Users</h1>


<form>
  #
</form>

<table border="1">
  <tr style="background-color: #008148">
    <th>
      ID
    </th>
    <th>
      Username
    </th>
    <th>
    </th>
  </tr>

  <%@list_of_users.each do |a_user|%>
    <tr>
      <td>
        <%=a_user.id%>
      </td>
      <td>
        <%=a_user.username%>
      </td>
      <td>
        <a href="/users/<%=a_user.id%>">Show Details</a>
      </td>
    </tr>
  <%end%>
</table>
